    "@fastify/swagger": "^7.6.1",
    "@fastify/view": "^7.1.0",
    "@fastify/websocket": "^7.1.0",
    "@google-cloud/storage": "^6.5.2",
    "@gquittet/graceful-server": "^4.0.1",
    "@toreda/adt": "^0.4.0",
    "ajv-i18n": "^4.2.0",
