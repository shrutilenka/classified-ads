{
  "name": "classified-ads",
  "version": "1.0.0",
  "description": "A performant classified-ads website with maps and tags",
  "main": "index.js",
  "scripts": {
    "build": "sudo apt-get install -y build-essential golang-go && go install github.com/mailhog/MailHog@v1.0.1 && npm install && sudo chmod -R a+rwx node_modules && cd client && npm install && npm run build && cd ..",
    "build:tools": "go install github.com/mailhog/MailHog@v1.0.1",
    "start": "node server",
    "dev:start": "~/go/bin/MailHog > '/dev/null' 2>&1 & node server.js",
    "dev:stop": "pkill --signal SIGINT classified-ads && kill -9 `pidof ~/go/bin/MailHog`",
    "dev:inpect": "~/go/bin/MailHog & node --inspect server.js",
    "dev:beautiful:inspect": "NO_CLUSTER=true clinic doctor -- node server.js",
    "dev:beautiful:inspect:2": "NO_CLUSTER=true clinic doctor --on-port 'autocannon localhost:3000' -- node server.js",
    "dev:beautiful:inspect:3": "NO_CLUSTER=true clinic bubbleprof --on-port 'autocannon -c 5 -a 500 localhost:3000' -- node server.js",
    "dev:tools": "cd tools && curl -L -O https://github.com/jcbvm/i18n-editor/releases/download/1.0.0/i18n-editor-1.0.0.zip --output i18n-editor-1.0.0.zip && unzip i18n-editor-1.0.0.zip",
    "tools:compress": "cd public/geo/ && json2geobuf states.min.json > states.min.pbf && json2geobuf simple_fr.json > simple_fr.pbf",
    "test:server": "NODE_ENV=api tap --no-coverage && ava",
    "test:unit": "ava test/units.js",
    "update_static": "license-report --only=prod --output=html --department.value= --relatedTo.value=  | tee  static/fr/third_parties.html static/dz/third_parties.html static/xx/third_parties.html > /dev/null",
    "update_other_apps": "cd other_apps && git clone https://github.com/bacloud23/mylinks.git"
  },
  "type": "module",
  "keywords": [
    "Node.js",
    "JavaScript",
    "MongoDB",
    "Maps",
    "web-app"
  ],
  "author": "bacloud23",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^7.5.0",
    "@fastify/auth": "^4.1.0",
    "@fastify/compress": "^6.1.1",
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.1.0",
