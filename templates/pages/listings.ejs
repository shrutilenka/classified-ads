<!DOCTYPE html>
<html>

<head>
  <%- include('../partials/shared/head'); %>
</head>

<body>
  <%- include('../partials/shared/navbar'); %>
  <div class="container">
    <div class="bg-light p-5 rounded">
      <h1><%= title %></h1>
      <p class="lead"><%= intro %></p>
      <a class="btn btn-lg btn-primary" href="/docs/5.0/components/navbar/" role="button">View navbar docs Â»</a>
    </div>

    <div class="nav-scroller py-1 mb-2" id="sync-top-tags"></div>
    <%- include('../partials/sections/geo_search_map'); %>
    <%- include('../partials/forms/gwoogl'); %>

    <hr>
    <h2 class="pb-2">Listings</h2>
    <% if(typeof section == 'undefined' && typeof context != 'undefined' && (context == 'geolocation' || context == 'gwoogl' || context == 'alllistings')){ %>
    <%- include('../partials/sections/big_listings_parser'); %>
    <% } else { %>
    <%- include('../partials/sections/listings_parser'); %>
    <% } %>
    <%- include('../partials/shared/pagination'); %>
    <%- include('../partials/sections/add_donation'); %>
    <%- include('../partials/sections/add_skill'); %>
    <%- include('../partials/sections/add_event'); %>
    <%- include('../partials/sections/add_blog'); %>
    <%- include('../partials/sections/add_hobby'); %>

    <%- include('../partials/conf/prepare_js'); %>
    <%- include('../partials/shared/footer'); %>
  </div>
  <%- include('../partials/conf/beforebodyclose'); %>
  <script>
    {
      if (window.__context__ === 'alllistings') {
        const scriptElement = document.createElement("script")
        scriptElement.src = "/javascripts/biglistings/biglists.js"
        document.body.appendChild(scriptElement)
      }
    }
  </script>
</body>

</html>