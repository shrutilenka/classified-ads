<form id="formControlRange0" style="background-color: #2a2a3c79; color: #fff;">
    <div class="form-group">
      <input type="range" class="form-control-range" id="formControlRange" min="1" max="8" step="1" value="1">
      <span id="rangeval">Day 1</span>
    </div>
  </form>
  <!-- this block can float in code, order doesn't matter -->
  <input id="pac-input" class="controls" style="width:50%; background-color: #E5EAF5" type="text" placeholder="<%= command %>" />
  <div id="infoWindow-content">
    <span id="place-name" class="title"></span><br />
    <!-- <strong>Place ID:</strong> <span id="place-id"></span><br /> -->
    <span id="place-address"></span>
  </div>
  <!-- this block can float in code, order doesn't matter -->
  <div class="empty-space" style="width: 100%; height: 5%;">
  
  </div>
  <div class="map_wrapper" id="stick" >
    <div id="map" style="width: 100%; height: 100%; " data-intro=<%- messages.tour[lang].map %> data-position="auto"></div>
  </div>
