            <li>
                <a data-trans="(categories)" href="/categories" class="nav-link px-2 text-secondary">Categories</a>
            </li>
            <li class="<%= typeof(section)!== 'undefined' && section === 'donations' && section %>">
                <a data-trans="(donations)" href="/listings/donations" class="nav-link px-2 text-white">Donations</a>
            </li>
            <li class="<%= typeof(section)!== 'undefined' && section === 'events' && section %>">
