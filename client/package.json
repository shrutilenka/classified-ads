{
  "name": "classified-ads-client",
  "version": "1.0.0",
  "description": "Not a standalone client. It only generates client scripts and styles that are served publicly",
  "source": [
    "src/views/main/index.js",
    "src/views/listings/listing.js",
    "src/views/biglistings/biglists.js",
    "src/views/skills/skills.js",
    "src/views/tags/tags.js",
    "src/views/weathervenue/weathervenue.js",
    "src/views/easteregg/easteregg.js"
  ],
  "targets": {
    "app": {
      "engines": {
        "browsers": "> 0.25%"
      },
      "distDir": "../public/javascripts/",
      "publicUrl": "./",
      "sourceMap": true
    }
  },
  "type": "module",
  "scripts": {
    "build": "find ../public/javascripts/ -type f -delete && parcel build && sass src/style:../public/stylesheets",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
