    "ad-rotator": "^5.7.0",
    "avatar-initials": "^5.0.0",
    "bootstrap": "^5.2.2",
    "clipboard": "^2.0.11",
    "detect-inapp": "^1.4.0",
    "dom-to-image": "^2.6.0",
    "font-picker": "^3.5.1",
    "holmes.js": "^1.17.3",
    "i18next": "^21.10.0",
    "i18next-browser-languagedetector": "^6.1.8",
    "i18next-http-backend": "^1.4.4",
    "leaflet": "^1.9.2",
    "leaflet.fullscreen": "^2.4.0",
